    "#use of if-elif-else statement\n",
    "marks=int(input(\"Enter marks\"))\n",
    "if(marks>=90):\n",
    "    print(\"Grade A\")\n",
    "elif(marks<90 and marks>=80):\n",
    "    print(\"Grade B\")\n",
    "elif(marks<80 and marks>=70):\n",
    "    print(\"Grade C\")\n",
    "else:\n",
    "    print(\"Grade D\")"
